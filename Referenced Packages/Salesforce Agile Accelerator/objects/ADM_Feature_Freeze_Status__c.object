    <validationRules>
        <fullName>Accessibility_details_required_bugs_open</fullName>
        <active>true</active>
        <description>When open Accessibility bugs are reported, details must be provided.</description>
        <errorConditionFormula>AND(ISPICKVAL(Accessibility_Bugs_Closed__c, &quot;No&quot;), ISBLANK( Provide_open_Accessibility_bug_details__c))</errorConditionFormula>
        <errorDisplayField>Provide_open_Accessibility_bug_details__c</errorDisplayField>
        <errorMessage>You must provide details when reporting open Accessibility bugs.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Details_must_be_provided_open_RCA_bugs</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(P0_1_2_Bugs_from_RCA_1_Month_Closed__c, &quot;No&quot;), ISBLANK(Provide_details_on_open_RCA_bugs__c))</errorConditionFormula>
        <errorDisplayField>Provide_details_on_open_RCA_bugs__c</errorDisplayField>
        <errorMessage>Details must be provided when reporting open RCA bugs that are older than one month.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Details_must_be_provided_open_RCAstories</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(RCA_Stories_1_Month_Closed_Scheduled__c, &quot;No&quot;), ISBLANK(Provide_details_on_open_RCA_Stories__c))</errorConditionFormula>
        <errorDisplayField>Provide_details_on_open_RCA_Stories__c</errorDisplayField>
        <errorMessage>Details must be provided when reporting RCA stories that are older than one month that are not Closed or Scheduled.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Details_must_be_provided_open_RCAtodos</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(RCA_To_Do_s_1_Month_Closed__c, &quot;No&quot;), ISBLANK(Provide_details_on_open_RCA_ToDos__c))</errorConditionFormula>
        <errorDisplayField>Provide_details_on_open_RCA_ToDos__c</errorDisplayField>
        <errorMessage>Details must be provided when reporting open RCA To Do that are older than one month.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Details_must_be_provided_open_Secur_bugs</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(P012_Security_Bugs_Closed__c, &quot;No&quot;), ISBLANK(Provide_details_on_open_Security_bugs__c))</errorConditionFormula>
        <errorDisplayField>Provide_details_on_open_Security_bugs__c</errorDisplayField>
        <errorMessage>Details must be provided when reporting open P0, P1 or P2 Security bugs.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Must_Explain_If_Not_Meeting_Criteria</fullName>
        <active>true</active>
        <errorConditionFormula>IF(ISBLANK( Team_Signoff_Status_Details__c),
   OR( ISPICKVAL( Team_Signoff_Status__c, &quot;Yes / does not meet criteria&quot;),
        ISPICKVAL( Team_Signoff_Status__c, &quot;No&quot;)),
    false)</errorConditionFormula>
        <errorDisplayField>Team_Signoff_Status_Details__c</errorDisplayField>
        <errorMessage>You must provide details if signing off without meeting criteria or when setting Signoff Status to &quot;No&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Must_Explain_if_Yellow_or_Red</fullName>
        <active>true</active>
        <errorConditionFormula>IF ( ISBLANK( Assessment_Details__c ), 
        IF(ISPICKVAL(Teams_Self_Assessment__c,  &apos;Red&apos;),true,
          IF(ISPICKVAL( Teams_Self_Assessment__c, &apos;Yellow&apos;),true,
      false)),false)</errorConditionFormula>
        <errorDisplayField>Assessment_Details__c</errorDisplayField>
        <errorMessage>You must provide details if Team status is Yellow or Red.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>One_2Week_Response_Only</fullName>
        <active>true</active>
        <errorConditionFormula>(EA_2Week_Risk_Response__c &amp;&amp; EA_2Week_NotReady_Response__c) || (EA_2Week_Ready_Response__c &amp;&amp; EA_2Week_Risk_Response__c) || (EA_2Week_Ready_Response__c &amp;&amp; EA_2Week_NotReady_Response__c)</errorConditionFormula>
        <errorMessage>Choose only one of the responses in the 2 Week Overall Assessment section.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>One_6Week_Response_Only</fullName>
        <active>true</active>
        <errorConditionFormula>(EA_6Week_Risk_Response__c &amp;&amp; EA_6Week_NotReady_Response__c) || (EA_6Week_Ready_Response__c &amp;&amp; EA_6Week_Risk_Response__c) || (EA_6Week_Ready_Response__c &amp;&amp; EA_6Week_NotReady_Response__c)</errorConditionFormula>
        <errorMessage>Choose only one of the responses in the 6-8 Week Overall Assessment section.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>One_6Week_Response_Required</fullName>
        <active>false</active>
        <errorConditionFormula>NOT(EA_6Week_Risk_Response__c) &amp;&amp; NOT(EA_6Week_NotReady_Response__c) &amp;&amp; NOT(EA_6Week_Ready_Response__c)</errorConditionFormula>
        <errorMessage>Choose one of the responses in the 6-8 Week Overall Assessment section.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Provide_details_when_dependencies_open</fullName>
        <active>true</active>
        <errorConditionFormula>IF(ISBLANK( Provide_other_team_dependencies_details__c ), 
    OR(
      AND(ISPICKVAL( Features_Complete_and_DoD_Achieved__c , &quot;No&quot;),
       NOT(ISPICKVAL( Dependencies_on_other_teams_closed__c , &quot;Yes&quot;))
      ),
      ISPICKVAL( Dependencies_on_other_teams_closed__c , &quot;No&quot;)
    ),
    false)</errorConditionFormula>
        <errorDisplayField>Provide_other_team_dependencies_details__c</errorDisplayField>
        <errorMessage>Details must be provided when dependencies on other teams are not closed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Provide_details_when_not_behind_perm</fullName>
        <active>true</active>
        <errorConditionFormula>IF(ISBLANK(  Provide_behind_perm_details__c  ), 
    OR(
      AND(ISPICKVAL( Features_Complete_and_DoD_Achieved__c , &quot;No&quot;),
       NOT(ISPICKVAL( Features_behind_perm_disabled__c , &quot;Yes&quot;))
      ),
      ISPICKVAL( Features_behind_perm_disabled__c , &quot;No&quot;)
    ),
    false)</errorConditionFormula>
        <errorDisplayField>Provide_behind_perm_details__c</errorDisplayField>
        <errorMessage>Details must be provided when feature is not behind perm and disabled when feature is not complete.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Scale_details_required_if_not_documented</fullName>
        <active>true</active>
        <description>Details must be provided if Scale Impact is not documented.</description>
        <errorConditionFormula>AND(ISPICKVAL(Scale_Impact_Documented__c, &quot;No&quot;), ISBLANK( Provide_Details_for_Scale_Impact__c))</errorConditionFormula>
        <errorDisplayField>Provide_Details_for_Scale_Impact__c</errorDisplayField>
        <errorMessage>You must provide details if scale impact has not been reviewed and documented.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Week2_NotReady_Comments_Required</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK(EA_6Week_NotReady_Comment__c) &amp;&amp; EA_2Week_NotReady_Response__c</errorConditionFormula>
        <errorDisplayField>EA_2Week_NotReady_Comment__c</errorDisplayField>
        <errorMessage>Enter next steps or assistance that is required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Week2_Risk_Comments_Required</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK(EA_2Week_Risk_Comment__c) &amp;&amp; EA_2Week_Risk_Response__c</errorConditionFormula>
        <errorDisplayField>EA_2Week_Risk_Comment__c</errorDisplayField>
        <errorMessage>Enter comments or assistance that is required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Week6_NotReady_Comments_Required</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK(EA_6Week_NotReady_Comment__c) &amp;&amp; EA_6Week_NotReady_Response__c</errorConditionFormula>
        <errorDisplayField>EA_6Week_NotReady_Comment__c</errorDisplayField>
        <errorMessage>Enter next steps or assistance that is required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Week6_Risk_Comments_Required</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK(EA_6Week_Risk_Comment__c) &amp;&amp; EA_6Week_Risk_Response__c</errorConditionFormula>
        <errorDisplayField>EA_6Week_Risk_Comment__c</errorDisplayField>
        <errorMessage>Enter comments or assistance that is required.</errorMessage>
    </validationRules>
