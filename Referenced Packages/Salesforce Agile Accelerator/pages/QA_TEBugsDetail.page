<apex:page standardController="agf__QA_Test_Execution__c"  extensions="agf.QA_TEBugDetailController" showHeader="true"><apex:detail relatedList="true"/>    <apex:form >    <a href="/apex/adm_workedit?Subject__c={!QA_Test_Execution__c.Test_Case__r.Brief_Description__c}&Scheduled_Build__c={!QA_Test_Execution__c.User_Story__r.Release__r.Name}&Description__c={!QA_Test_Execution__c.Test_Case__r.Steps_to_Reproduce__c}&Email_Subscription__c={!QA_Test_Execution__c.Assignee__r.EMail}" id="theLink" >Add Bug</a>           <p/><apex:pageBlock title="Bugs" id="block">                <apex:outputLabel value="{!bugs}" rendered="{!(ISNULL(bugs))}">                        <strong>No Bug Records associated to this Test Execution</strong>                    </apex:outputLabel>                    <apex:pageBlockTable value="{!bugs}" var="bugsl" rows="100" width="100%" rendered="{!NOT(ISNULL(bugs))}">                                <apex:column width="10%">                                    <apex:facet name="header">                                      <apex:commandLink action="{!orderBugNumber}" value="Work ID {!sortDirection}" rerender="block"/>                                    </apex:facet>                                    <a href = "/{!bugsl.id}" target="_blank"><font color="#3300FF"><b>{!bugsl.Name}</b></font></a>                                </apex:column>                                <apex:column width="35%" value="{!bugsl.agf__Subject__c}"/>                                <apex:column width="5%" value="{!bugsl.agf__Priority__c}"/>                                <apex:column width="15%" value="{!bugsl.agf__Status__c}"/>                                <apex:column width="20%" value="{!bugsl.agf__QA_Engineer__c}"/>                                <apex:column width="20%" value="{!bugsl.agf__Assignee__c}"/>                                <apex:column width="5%" value="{!bugsl.agf__Scheduled_Build__c}"/>                    </apex:pageBlockTable>                    <apex:outputLabel rendered="{!NOT(ISNULL(bugs))}">                       <br/>                       <h3> {!bugsListSize} </h3> Bug Record(s) associated to this Test Execution                    </apex:outputLabel>        </apex:pageBlock>    </apex:form>    <style>        .fewerMore { display: none;}    </style></apex:page>