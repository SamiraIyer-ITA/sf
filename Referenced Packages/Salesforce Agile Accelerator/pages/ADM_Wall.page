gus.LookupDialog = Backbone.View.extend({
    events: {
        'dialogclose': 'close'
    },
    initialize: function(options) {
        options = _.defaults({}, options, {title: 'Lookup'});
        this.title = options.title;
        this.modelClass = options.modelClass;
        this.label = options.label;
        this.template = _.template($('#template-lookup-dialog').html());
        this.errorMessage = null;
        this.isOpen = false;
        
        //create the component that does the lookup
        this.nameComponent = options.nameComponent || new gus.LookupComponent({
            modelClass: this.modelClass
        });
        this.nameComponent.on('keypress', this._onInputKeyPress, this);
        this.nameComponent.on('error', function(errorMessage) {
            this._onError(errorMessage);
        }, this);
        
        //bind events
        this.on('error', this._updateError, this);
        this.on('clear-error', this._updateError, this);
    },
    render: function() {
        //since jquery dialog creates a new DOM at the body level, we override
        //the standard element within backbone with the new dialog dom
        var dialog = this._renderDialog();
        this.setElement(dialog);
        
        this.$('label').text(this.label);
        
        //render the input component and add it to the container 
        this.$('.input-container').empty().append(this.nameComponent.render().$el);
        
        this._updateError();
        return this;
    },
    clear: function() {
        this.nameComponent.clear();
        this._clearError();
        return this;
    },
    remove: function() {
        if(this.nameComponent) {
            if(this.nameComponent.remove) {
                this.nameComponent.remove();
            }
            delete this.nameComponent;
        }
        
        if(this.$el && this.$el.data('dialog')) {
            this.$el.dialog('destroy');
        }
        
        gus.LookupDialog.__super__.remove.apply(this, arguments);
    },
    open: function() {
        if(this.isOpen) {
            return;
        }
        this.isOpen = true;
        if(this.$el && this.$el.data('dialog')) {
            this.$el.dialog('open');
        }
        return this;
    },
    close: function() {
        if(!this.isOpen) {
            return;
        }
        this.isOpen = false;
        if(this.$el && this.$el.data('dialog')) {
            this.$el.dialog('close');
        }
        this.trigger('close');
        return this;
    },
    getSelected: function() {
        return this.selected;
    },
    setSelected: function(value) {
        this.selected = value;
        this.trigger('select', value);
    },
    _onError: function(msg) {
        this.errorMessage = msg;
        console.warn(msg);
        this.trigger('error');
    },
    _clearError: function() {
        this.errorMessage = null;
        this.trigger('clear-error');
    },
    _updateError: function() {
        if(this.errorMessage) {
            this.$('.field-error').removeClass('ui-helper-hidden');
            this.$('.field-error-message').text(this.errorMessage);
        } else {
            this.$('.field-error').addClass('ui-helper-hidden');
            this.$('.field-error-message').empty();
        }
    },
    _renderDialog: function() {
        return $(this.template()).dialog({
            modal: true,
            autoOpen: false,
            title: this.title,
            resizable: false,
            buttons: [
                {text:"OK", click: _.bind(this._dialogOk, this)},
                {text:"Cancel", click:_.bind(this._dialogCancel, this)}
            ]
        });
    },
    _dialogOk: function() { 
        var dialog = this;
        this._clearError();
        this.nameComponent.commit()
            .done(function() {
                var selectedId = dialog.nameComponent.getId();
                if(selectedId) {
                    gus.getCache(dialog.modelClass).getOrFetch(selectedId)
                        .done(function(model) {
                            dialog.setSelected(model);
                            dialog.close();
                        })
                        .fail(function(event) {
                            dialog._onError('Unable to find model with the specified ID: ' + selectedId, event);
                        });
                } else {
                    dialog._onError('Unable to find a ' + (dialog.label || 'object')+ ' with the name entered.');
                }
            })
            .fail(function(component, message) {
                dialog._onError('Unable to find a ' + (dialog.label || 'object')+ ' with the name entered.\n' + message);
            });
    },
    _dialogCancel: function() {
        this.$el.dialog('close');
    },
    _onInputKeyPress: function(e) {
        if(e.keyCode == 13) {
            this._dialogOk();
        }
    }
});

