    },
    
    unfill: function() {
        gus.WorkListPanel.__super__.unfill.apply(this, arguments);
        this.$el.removeClass('panel-fill');
        this.recalcHeight();
    },
    
    recalcHeight: function() {
        if(!this.panelContainer) {
            return;
        }
        
        var loadingVisible = this.$('.panel-loading').is(':visible');
        var workList = this.$('.wall-work-list');
        if (loadingVisible) {
            workList.height(0);
        } else if (!loadingVisible && workList.outerHeight() <= 30) {
            workList.height(30);
        } else if (!loadingVisible && workList.outerHeight() > 30) {
            workList.css('height','auto');
        }
    
        var maxPanelHeight = this.panelContainer.maxPanelHeight;
        var panelHeaderHeight = this.$('.panel-header').outerHeight();
        var panelOffset = this.$el.outerHeight() - this.$el.height();
        this.$('.table-container').height(maxPanelHeight - panelHeaderHeight - panelOffset);

        var addedUp = maxPanelHeight - panelHeaderHeight - panelOffset;
        
        var contentHeight = this._getContentHeight();
        if ( contentHeight < this.$('.table-container').height() ) {
            this.$('.table-container').height(contentHeight);
            this.$('.table-container').removeClass('ui-scroll-helper');
        } else {
            this.$('.table-container').addClass('ui-scroll-helper');
        }

        /*if (this.collection.length > 0) {
            this.$('.table-container').removeClass('ui-noresults-helper');
        } else {
            this.$('.table-container').addClass('ui-noresults-helper');
        }*/
    },
    
    showMore: function() {
        var deferred = new $.Deferred();
        if(_.isUndefined(this.collection.hasMoreRecords) || _.result(this.collection, 'hasMoreRecords') === false) {
            deferred.resolve(false);
            return deferred.promise();
        }
        
        console.log('Loading the next batch of records');
        
        this.$('.panel-show-more').hide();
        this.$('.panel-loading').show();
        this.recalcHeight();
        
        this.collection.fetchNextBatch()
            .always(_.bind(function(event) {
                this.$('.panel-loading').hide();
                this.recalcHeight();
                deferred.resolve(true);
            }, this))
            .fail(_.bind(function(event) {
                this._onError('Failed to load records', event);
                deferred.reject('Failed to load records', event);
            }, this));
            
        return deferred.promise();
    },
    
    _getContentHeight: function() {
        var contentHeight = this.$('.wall-work-list').outerHeight();
        
        //if the show more link is visible, we need to account for it in the scrollable area
        var showMoreVisible = this.$('.panel-show-more').is(':visible');
        if(showMoreVisible) {
            contentHeight += this.$('.panel-show-more').outerHeight();
        }
        
        var loadingVisible = this.$('.panel-loading').is(':visible');
        if(loadingVisible) {
            contentHeight += this.$('.panel-loading').outerHeight();
        }
        
        return contentHeight;
    },
    
    sprintToggle: function(event) {  
   $('#btn-sprinttoggle button').attr('disabled','true');      
   hideWorkWithSprint = (hideWorkWithSprint == 'false') ? 'true' : 'false'; 
   this.refreshPanel();
    },
    
    _onShowColumnSettingsButtonClick: function(event) {
        event.preventDefault();
        event.stopPropagation();
        
        if(this.$('.menu_columns').is(':visible')) {
            this.hideColumnSelectionMenu();
        } else {
