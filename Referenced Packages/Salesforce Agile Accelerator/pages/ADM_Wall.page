gus.QueryString = function(values) {
    this.values = (values || {});
};
_.extend(gus.QueryString.prototype, {
    containsKey: function(key) {
        return this.values.hasOwnProperty(key);
    },
    get: function(key) {
        if(!this.containsKey(key)) {
            return void 0;
        }
        return this.values[key];
    },
    set: function(key, value) {
        this.values[key] = value;
    },
    toString: function() {
        var keyValuePairs = [];
        for(var key in this.values) {
            keyValuePairs.push(encodeURI(key) + '=' + encodeURI(this.values[key]));
        }
        return keyValuePairs.join('&');
    }
});

/**
 * Parses the query string into a key-value pair object.
 */
gus.QueryString.parse = function(queryStringValue) {
    var args = queryStringValue.split('&');
    var parsed = {};
    
    for(var index = 0; index < args.length; index++) {
        var arg = decodeURI(args[index]);
        
        var key, value;
        
        if(arg.indexOf('=') === -1) {
            key = arg;
            value = true;
        } else {
            var keyValuePair = arg.split('=');
            key = keyValuePair[0];
            value = keyValuePair[1];
        }
        
        parsed[key.trim()] = value;
    }
    return new gus.QueryString(parsed);
};

