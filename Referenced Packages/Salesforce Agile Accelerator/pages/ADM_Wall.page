    showWorkDialog: function(workModel) {
        
        if (this.workDialog) {
            return;
        }
    
        workModel.getRecordType().done(_.bind(function(recordType) {
            var dialog = null;
            var name = recordType.get('Name');
            if(name === 'Bug') {
                dialog = new gus.WorkEditorBug({model: workModel, teamId: this.teamId});
            } else if(name === 'User Story') {
                dialog = new gus.WorkEditorUserStory({model: workModel, teamId: this.teamId});
            } else if(name === 'ToDo') {
                dialog = new gus.WorkEditorToDo({model: workModel, teamId: this.teamId});
            } else {
                this._onError('Unsupported record type ' + name + ' specified.');
                return;
            }
            
            this.workDialog = dialog.render();
            this.workDialog.open();
            this.workDialog.on('close', _.bind(function() {
                this.workDialog.remove();
                this.workDialog = null;
            }, this));
        }, this));
    },

    addPanel: function(panel) {
        var panelContainer = this;
        this.panels.push(panel);
        panel.setPanelContainer(this);
        var $panelList = this.$('#panel-list').append(panel.render().el);
        
        panel.on('removed', function() {
            panelContainer.removePanel(panel);
        });
        panel.on('change:index', function(panel, index) {
        
            //move the panel within the array
            panelContainer.panels = _.without(panelContainer.panels, panel);
            panelContainer.panels.splice(index, 0, panel);
            
            //move the panel in the UI
            var $panelUiElements = $panelList.find('>');
            var currentUiIndex = $panelUiElements.index(panel.$el);
            if(currentUiIndex !== index) {
                $panelUiElements.eq(index).after($panelUiElements.eq(index - 1));
            }
            
            panelContainer.trigger('panel:moved', panelContainer, panel, index);
        });
        
        // unfill any currently filled panels to make room for new panel
        _.each(this.panels, function(panel) {
            if(panel.isFill) {
                panel.unfill();
            }
        });
        
        panel.show();
        
        panel.el.scrollIntoView();
        
        this.trigger('panel:added', panelContainer, panel);
        
        return this;
    },
    
    removePanel: function(panel) {
        
        //if the panel is filled, then we want to unfill it before removing it
        if(_.result(panel, 'isFill')) {
            panel.unfill();
        }
        
        //whenever a panel is removed, we want to remove it from the panel collection
        if(_.contains(this.panels, panel)) {
            this.panels = _.without(this.panels, panel);
        }
        
        this.trigger('panel:removed', panelContainer, panel);
        
        return this;
    },
    
