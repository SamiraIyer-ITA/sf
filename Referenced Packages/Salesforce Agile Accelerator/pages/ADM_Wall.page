                    panelContainer.team = team;
                })
                .fail(function() {
                    panelContainer._onError('Team ID specified is not valid: ' + options.teamId);
                });
        
        //create object that loads the panels
        var panelRouter = new gus.PanelRouter({
            panelContainer: this,
