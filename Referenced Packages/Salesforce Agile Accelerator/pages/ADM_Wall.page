        }
        
        var themeDeferred = $.Deferred().resolve(theme);
        var panel = new gus.ThemePanel({
            themeDeferred: themeDeferred,
            teamDeferred: this._fetchTeam()
        });
        this.addPanel(panel);
        return $.Deferred().resolve(panel);
