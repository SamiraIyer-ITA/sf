        }).render();
        
        var errorHandler = _.bind(function(message) {
            return _.bind(function() {
                this._dialogCancel();
                this.$('.workEditorLoader').text(message).css('background','transparent');
                console.warn(message);
            },this);
        },this);

        var recordTypePromise = this.model.getRecordType()
            .done(_.bind(function(recordType) {
                gus.WorkModel.getStatusValues(recordType.get('Name')).done(_.bind(function(statusValues) {
                    var $statusInput = this.$('#statusInput').empty();
                    _.each(statusValues, function(statusValue) {
                        var $option = $('<option>').text(statusValue).attr('value', statusValue).appendTo($statusInput);
                        if (view.model.get(nameSpace+'Status__c') == statusValue) { 
                            $option.attr('selected', 'selected');
                        }
                    });
                }, this));

                this.$('#statusInput option[value="' + this.model.get('Status__c') + '"]').attr('selected', 'selected');
            }, this))
            .fail(errorHandler('Failed to load record types: ' + this.model.get('RecordTypeId')));

        this.$('#subjectInput').val(this.model.get(nameSpace+'Subject__c'));
        var assigneePromise = this.model.getAssignee()
            .done(function(assignee) {
                if (assignee) {
                    view.userLookupComponent.select(assignee.toJSON());
