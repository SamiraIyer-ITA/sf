    _createPanelsFromPanelArgs: function(panelArgs) {
        var panelContainer = this.panelContainer;
        var panelDeferreds = [];
        for(index = 0; index < panelArgs.length; index++) {
            var panelArg = panelArgs[index];
            
            //if no route defined for the ID, then we don't need to continue processing
            if(!this.panelDefsById.hasOwnProperty(panelArg.id)) {
                console.warn('Unknown panel ID specified: %s', panelArg.id);
                continue;
            }
            
            var panelDef = this.panelDefsById[panelArg.id];
            
            //create a panel for each panelDefinition specified 
            var deferred = panelDef.fromPanelArg(panelArg, {
                panelContainer: panelContainer,
                teamDeferred: this.teamDeferred
            });
            
            //if there is a failure when loading the panel, then we need to show the error
            deferred.fail(function(event){
                if(event.message) {
                    panelContainer._onError(event.message);
                }
            });
            
            panelDeferreds.push(deferred);
        }
        
        //after we have all of the panels starting to load, we want to 
        //add the panels to the panelContainer in the order that they were 
        //specified.
        return this._whenAll(panelDeferreds).done(function(panels) {
            _.each(panels, function(panel) {
                //if a panel fails to load, the value in the array will be blank
                //so we want to skip the blanks
                if(panel) {
                    panelContainer.addPanel(panel);
                }
            });
        });
    },
    
    //returns a new deferred that resolves when all of the given deferreds have resolved.
    //if any of the deferreds are rejected, the returned deferred is still resolved however the 
    //index within values array is blank for the rejected deferreds.
    //this is different than $.when because $.when is rejected when any deferred is rejected.
    _whenAll: function(deferreds) {
        var allDeferred = $.Deferred(),
            
            //counts the number of deferreds left to resolve
            remaining = deferreds.length,
            
            //stores all of the values that have been loaded
            values = new Array(deferreds.length),
            
            //callback to set the value in the array
            setValueAt = function(index) {
                return function(value) {
                    values[index] = value;
                };
            },
            
            //callback to decrement the remaining and resolve when finished
            updateContext = function(index) {
                return function() {
                    //decrement the remaining value and resolve if all deferreds have
                    //been resolved.
                    if((--remaining) === 0) {
                        allDeferred.resolve(values);
                    }
                };
            };
         
        //register the callbacks for each of the deferreds given
        for(index = 0; index < deferreds.length; index++) {
            deferreds[index]
                .done(setValueAt(index))
                .always(updateContext(index));
        }
        
        return allDeferred;
    },
    _updateHistory: function() {
        //Every time panels are added, removed or moved from one side to another this function is called and it has the current location and  set of panels the user sees.
        var root = document.location.pathname;
        var queryString = gus.QueryString.parse(document.location.search.substring(1));
        queryString.set('panels', this._toQueryString());
        var panelsToSave = this._toQueryString();  //Need to save these to JSON field.
        var panelsArray = null;
        if(panelsToSave != ""){
             panelsArray = panelsToSave.split('|');
        }
        else{
            panelsArray = [];
        }
        //This is done because the myData is a global object being manipulated everywhere. We do not know currently if it is a string or an object because everytime a change is made it is saved back to myData. Also .parse() throws an error if it of typer object.
        if(typeof myData != 'object'){        
            myData = JSON.parse(myData.toString());
        }
        $.each(myData.user.teams,function(){
            if(this.id == uwTeamId){
                
                if(this.panels.length < panelsArray.length){   //Case of Adding a new Panel
                this.panels.push({"name" : panelsArray[panelsArray.length-1], "order" : this.panels.length,"columns" : []});

                }
                else if(this.panels.length > panelsArray.length){ // Case of closing a panel
                var count = -1;
                $.each(this.panels,function(){
                count= count+1;
                if(panelsArray.indexOf(this.name) == -1){
                return false;
                }
                });
                if(count < this.panels.length){
                var array1 = this.panels.splice(count,1);
                }
                }
                else if(this.panels.length == panelsArray.length){  //Case of moving a panel
                $.each(this.panels,function(){
                for(var i=0;i<panelsArray.length;i++){
                if(panelsArray[i] == this.name){
                this.order = i;
                }
                }

                });
                }     
            }
        });
        myData = JSON.stringify(myData); // Convert it back to JSON frm javascript object.
        gus.setJSONValues(myData) //save to database.
                .fail(function(event) {
                    panel._onError('Failed to save JSON values', event); 
                });
    },
    _toQueryString: function() {
        var panels = this.panelContainer.panels;                     
        var panelArgs = this._panelsToPanelArgs(panels);
        var states = [];
        for(var index = 0; index < panelArgs.length; index++) {
            var panelArg = panelArgs[index];
            
            //convert the panelArg to a string
            if(panelArg.state) {
                states.push(panelArg.id + '(' + panelArg.state + ')');
            } else {
                states.push(panelArg.id);
            }
        }

        return states.join(this.panelDelimiter);
    },
    
    //given an array of panels, get an array of panelArgs
    _panelsToPanelArgs: function(panels) {
        var panels = this.panelContainer.panels;
        var panelArgs = [];
        for(var index = 0; index < panels.length; index++) {
            var panel = panels[index];
            
            //find the panel definition for the panel in the container
            var panelDef;
            for(var panelId in this.panelDefsById) {
                var thisPanelDef = this.panelDefsById[panelId];
                if(thisPanelDef.isInstanceOf && thisPanelDef.isInstanceOf(panel)) {
                    panelDef = thisPanelDef;
                    break;
                }
            }
            
            if(!panelDef) {
                console.warn('Unable to get panel definition for the panel %s', index);
                continue;
            }
            
            //get the panelArg for the the panel from the panel definition
            var panelArg;
            if(panelDef.toPanelArg) {
                panelArg = panelDef.toPanelArg(panel);
            } else {
                console.warn('Unable to save panel because panel definition %s does not contain toPanelArg', panelDef.id);
                continue;
            }
            
            panelArgs.push(panelArg);
        }
        return panelArgs;
    },
    
    //given a string, an array of panel args are returned.
    _parseStringToPanelArgs: function(value) {
        //parse the panels argument
        var panelArgs = value.split(this.panelDelimiter);
        
        //parse each panel initialization
        var panels = [];
        for(var index = 0; index < panelArgs.length; index++) {
        
            try {
                var panelArg = panelArgs[index];
                var panelId, state;
                if(panelArg.indexOf('(') === -1) {
                    panelId = panelArg.trim();
                } else {
                    panelId = panelArg.substring(0, panelArg.indexOf('(')).trim();
                    state = panelArg.substring(panelArg.indexOf('(') + 1, panelArg.indexOf(')'));
                }
                
                panels.push({
                    id: panelId, 
                    state: state
                });
            } catch(err) {
                console.error('Unable to parse panel argument: ' + panelArg);
                console.log(err);
            }
        }
        return panels;
    }
});

gus.Panel = Backbone.View.extend({
    tagName: 'li',
    className: 'panel',
    
    initialize: function(options) {
        gus.Panel.__super__.initialize.apply(this, arguments);
    },
    
    setPanelContainer: function(panelContainer) {
        this.panelContainer = panelContainer;
    },
    
    remove: function() {
        gus.Panel.__super__.remove.apply(this, arguments);
        this.trigger('removed');
    },
    
    show: function() {
        this.$el.removeClass('ui-helper-hidden');
    },
    
    hide: function() {
        this.$el.addClass('ui-helper-hidden');
    },
    
    isFill: false,
    
    /**
     * Hides all of the panels except this panel and expands this panel to fill all of the panel container space.
     */
    fill: function() {
        if(this.isFill) {
            return;
        }
        
        //unfill any other panel that may already be filling the panel
        _.each(_.where(this.panelContainer.panels, function(panel) {return panel.isFill;}), function(panel) {
            panel.unfill();
        });
        
        this.isFill = true;
        this.$el.addClass('panel-fill').data('fill', true);
        
        //reset all of the other panels
        _.each(_.without(panelContainer.panels, this), function(panel) {
            panel.hide();
        });
        
        //show only the current panel
        this.show();
        
        this.trigger('change:fill');
    },
    
    /**
     * If this panel is expanded (using fill), it reverts to the previous size and shows all of the other panels.
     */
    unfill: function() {
        if(!this.isFill) {
            return;
        }
        
        this.isFill = false;
        this.$el.removeClass('panel-fill').data('fill', false);
        
        //unfill all of the panels
        _.each(_.without(panelContainer.panels, this), function(panel){
            panel.show();
        });
        
        this.trigger('change:fill');
        panelContainer.resize();
    },
    
    /**
     * If the panel is currently not filling the container, then it fills otherwise it unfills.
     */
    toggleFill: function() {
        if(!this.isFill) {
            this.fill();
        } else {
            this.unfill();
        }
    },

    delegateEvents: function() {
        this.events = _.defaults(this.events, {
            'drop': '_onPanelDrop'
        });
        gus.Panel.__super__.delegateEvents.apply(this, arguments);
    },
    
    /**
     * Invoked when the DOM element is dropped
     */
    _onPanelDrop: function(event, index) {
        this.trigger('change:index', this, index);
    },

    /**
     * Used to store items being dragged for resorting
     */
    itemsBeingSorted: null,

    /**
     * Used to store Backbone models being dragged for resorting
     */
    modelsBeingSorted: null,

    /**
     * Used to store where multi-item sort items
     * were dropped
     */
    droppedIndex: null,

    /**
     * Used to store which panel the dropped
     * items landed on
     */
    droppedPanel: null,
    numberOfRecordsSelected : null
});

