    _renderAddPanelMenu: function() {
        var buttonToDomMappings = {
            'backlog': '#button-backlog',
            'backburner': '#button-backburner',
            'epic'      : '#button-epic',
            'featureBacklog': '#button-feature-backlog',
            'trustBacklog': '#button-trust-backlog',
            'sprintBacklog': '#button-sprint',
            'themeBacklog': '#button-theme',
            'buildBacklog': '#button-build'
        };
        
        //create a view for each button
        _.each(this.addPanelMenu.buttons, _.bind(function(buttonModel) {
            var $buttonDom = this.$(buttonToDomMappings[buttonModel.id]);
            
            var buttonView = new gus.menu.MenuItemButtonView({
                model: buttonModel,
                $el: $buttonDom
            }).render();
        }, this));
    },
    
    _initializeAddPanelMenu: function() {
        var addPanelMenu = this.addPanelMenu;
        
        //return back a callback that disables the specified button when the panel is of the specified type
        var disableButton = function(buttonId, panelType) {
            var button = addPanelMenu.get(buttonId);
            return function(panelContainer, panel) {
                if(!(panel instanceof panelType)) {
                    return;
                }
                panel.on('removed', function() {
                    button.enable();
                });
                button.disable();
            };
        };
        
        //disable the addPanelMenu button when the a panel is added
        this.on('panel:added', disableButton('backlog', gus.MasterBacklogPanel));
        this.on('panel:added', disableButton('backburner', gus.BackburnerPanel));
        this.on('panel:added', disableButton('featureBacklog', gus.FeatureBacklogPanel));
        this.on('panel:added', disableButton('trustBacklog', gus.TrustBacklogPanel));
    },
    
    /**
     * Updates the UI/DOM for the "Add Panel" menu
     */
    _updateAddPanelMenu: function() {
        if(this.addPanelMenu.visible) {
            //make sure all of other menus are hidden
            this.hideDropdowns();
            
            //show the menu and position under the button
			if (currentUserLightningDesktopPreference) {
	            this.$('.menu_addpanel').show().position({my:'right top', at:'right bottom', of:this.$('button#btn-add-panel'), offset:'0px -1px'});
			} else {
	            this.$('.menu_addpanel').show().position({my:'left top', at:'left bottom', of:this.$('#btn-add-panel button'), offset:'0px -1px'});
			}
        } else {
            this.$('.menu_addpanel').hide();
        }
    },
    
    /**
     * Updates the UI/DOM for the "Add Panel" button
     */
    _updateAddPanelButton: function() {
        var $button = this.$('#btn-add-panel button');
        if(this.addPanelMenu.visible) {
            $button.addClass('on');
            $button.find('div.icon').addClass('icon-white');
            $button.find('div.arrow-down').addClass('arrow-down-white');
        } else {
            $button.removeClass('on');
            $button.find('div.icon').removeClass('icon-white');
            $button.find('div.arrow-down').removeClass('arrow-down-white');
        }
    },
    
    _onShowAddPanelButtonClick: function(event) {
        event.preventDefault();
        event.stopPropagation();
        
        if(this.addPanelMenu.visible) {
            this.hideAddPanelMenu();
        } else {
            this.showAddPanelMenu();
        }
    },
    
    /**
     * Gets the team for the team dependent panels
     */
    _fetchTeam: function() {
        return this.teamDeferred;
    },
    
    _onError: function(message, event) {
        //show a jquery modal dialog
        var $el = $('<div><div class="error-dialog-message"></div><div class="error-dialog-details" style="display: none; margin-top: 1em;"></div></div>');
        
        $el.find('.error-dialog-message').text(message);
        if(event && event.message) {
            $el.find('.error-dialog-details').show().text(event.message);
        }
        
        $el.dialog({
            title: 'Error Occurred',
            modal: true,
            buttons: {
