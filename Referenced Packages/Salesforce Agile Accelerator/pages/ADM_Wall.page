gus.TaskColumns = Backbone.View.extend({
    initialize: function() {
        this.columns = [
            new gus.TaskCardColumn(),
            new gus.TaskCardColumn(),
            new gus.TaskCardColumn({
                maxCards: 4
            })
        ];
    },
    render: function() {
        _.invoke(this.columns, 'render');
        this.columns[0].$el.appendTo(this.$('.work-tasks-column1 .task-list'));
        this.columns[1].$el.appendTo(this.$('.work-tasks-column2 .task-list'));
        this.columns[2].$el.appendTo(this.$('.work-tasks-column3 .task-list'));
        
        //if any of the columns change their stack property,
        //let's just send it up
        _.each(this.columns, _.bind(function(column) {
            column.on('change:stack', function(){
                this.trigger('change:stack');
            }, this);
        }, this));
        
        return this;
    },
    setTasks: function(tasks) {
        //we need to sort the tasks into three different columns
        var columns = {
            'Not Started': [],
            'In Progress': [],
            'Completed': []
        };
        
        //sort all of the tasks between the three columns
        _.each(tasks, function(task) {
            if(columns[task.get(nameSpace + 'Status__c')]) {
                columns[task.get(nameSpace + 'Status__c')].push(task);
            }else {
                columns['Not Started'].push(task);
            }
        });
        
        //add the new task cards
        this.columns[0].collection.reset(columns['Not Started']);
        this.columns[1].collection.reset(columns['In Progress']);
        this.columns[2].collection.reset(columns['Completed']);
    }
});

gus.TaskCardColumn = Backbone.View.extend({
    tagName: 'ul',
    events: {
        'click .showAllTasks a': '_onToggleStackClick'
    },
    initialize: function(options) {
        options = options || {};
        this.taskCards = options.taskCards;
        this.maxCards = options.maxCards || 9;
        this.showAllTemplate = _.template('<li class="showAllTasks"><a href="#"></a></li>');
        this.collection = new gus.TaskList();
        this.collection.comparator = function(task1, task2) {
            var order1 = task1.get('Order__c');
            var order2 = task2.get('Order__c');
            
            if(_.isUndefined(order1) && _.isUndefined(order2)) {
                return 0;
            } else if(_.isUndefined(order1)) {
                return 1;
            } else if(_.isUndefined(order2)) {
                return -1;
            }
            
            if(order1 < order2) {
                return -1;
            } else if(order1 > order2) {
                return 1;
            }
            return 0;
        };
        this.collection.on('reset', _.bind(this.reset, this));
    },
    reset: function() {
        this.taskCards = [];
        this.collection.each(_.bind(function(task) {
            var taskCard = new gus.TaskCard({
                model: task
            }).render();
            taskCard.on('remove', _.bind(function() {
                this.collection.remove(task);
            }, this));
            this.taskCards.push(taskCard);
        }, this));
        
        if(this.isStackable()) {
            this.stack();
        } else {
            this.unstack();
        }
    },
    toggleStack: function() {
        if(this.stacked) {
            this.unstack();
        } else {
            this.stack();
        }
    },
    isStackable: function() {
        return this.collection.length > this.maxCards;
    },
    unstack: function() {
        var beforeStacked = this.stacked;
        this.stacked = false;
        this.$el.empty().removeClass('stacked');
        _.each(this.taskCards, _.bind(function(taskCard) {
            this.$el.append(taskCard.el);
        }, this));
        
        //if this card can be stacked, show the "collapse" link
        if(this.isStackable()) {
            this.$el.append($(this.showAllTemplate())).find('.showAllTasks a').text('Collapse ' + this.taskCards.length + ' tasks');
        }
        
        if(beforeStacked != this.stacked) {
            this.trigger('change:stack');
        }
    },
    stack: function() {
        var beforeStacked = this.stacked;
        this.stacked = true;
        this.$el.empty();
        if(this.collection.length === 0) {
            return;
        } 
        
        this.$el.addClass('stacked');
        
        //show three blank task cards under the last card
        var $blankTaskCard = $('<li class="taskcard-blank"></li>');
        this.$el.append($blankTaskCard.clone());
        this.$el.append($blankTaskCard.clone());
        this.$el.append($blankTaskCard.clone());
        
        //show the last task card
        var lastTaskCard = this.taskCards[this.taskCards.length - 1];
        this.$el.append(lastTaskCard.el);
        
        //show the "show all tasks" link
        this.$el.append($(this.showAllTemplate())).find('.showAllTasks a').text('Expand ' + this.taskCards.length + ' tasks');
        
        if(beforeStacked != this.stacked) {
            this.trigger('change:stack');
        }
    },
    _onToggleStackClick: function(event) {
        event.preventDefault();
        this.toggleStack();
    }
});

