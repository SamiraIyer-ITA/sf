        this.$saveButton = this.$('#saveButton');
        this.$saveButtonText = this.$('#saveButton span.ui-button-text');
        this.$cancelButton = this.$('#cancelButton');

        return this;
    },
    remove: function() {
        var body = this._getDialogBody();
        if(body) {
            body.dialog('destroy');
        }
        gus.WorkEditor.__super__.remove.apply(this, arguments);
    },
    open: function() {
        var body = this._getDialogBody();
        if(body) {
            body.dialog('open');
        }
        return this;
    },
    close: function() {
        var body = this._getDialogBody();
        if(body) {
            body.dialog('close');
        }
        this.trigger('close');
        return this;
    },
    _openExternalLink: function(event) {
        event.preventDefault();
        this._dialogCancel();
        window.open('/' + this.model.get('Id'));
    },
    _renderDialog: function() {
        var dialogBody = $(this.template()).dialog({
            modal: true,
            autoOpen: false,
            title: this.title,
            resizable: false
        });
        
        return dialogBody.parents('.ui-dialog');
    },
    _getDialogBody: function() {
        return (this.$el && this.$('.ui-dialog-content').data('dialog')) ? this.$('.ui-dialog-content') : void 0;
    },
    _dialogOk: function() {
        this._onSave();
    },
    _dialogCancel: function() {
        var body = this._getDialogBody();
        body.dialog("close");
    },
    _showErrorIndicator: function() {
        var view = this;
        this.$formIndicator.removeClass('submitIndicator').addClass('errorIndicator').delay(1500).queue(function() {
            view.$formElements.removeAttr('disabled');
            view.$richTextEditor.attr('contenteditable','true');
            view.$saveButtonText.text('Save');
            view.$cancelButton.show();
            $.dequeue(this);
        }).delay(3000).queue(function() {
            view.$formIndicator.fadeOut('slow');
            $.dequeue(this);
        });
    },
    _showFormErrors: function(pageErrors) {
        this._showErrorIndicator();
        
        //display the page errors
        try {
            var errorContainer = this.$('.page-error-container').empty().hide();
            if(pageErrors.length > 1) {
                var pageErrorList = $('<ul>').appendTo(errorContainer);
                _.each(pageErrors, function(pageError) { $('<li>').text(pageError.message).appendTo(pageErrorList); });
            } else {
                errorContainer.text(pageErrors[0].message);
            }
            if(pageErrors.length > 0) {
                errorContainer.show();
            }
            
        } catch(e) {
            console.warn(e);
            console.error('Error occurred while displaying errors.');
            console.log(pageErrors);
        }
    },
    _onSaveError: function(model, xhr, options) {
        var pageErrors;
        
        if(xhr.readyState == 0 || xhr.status == 0) {
            console.warn('url changed during save');
            pageErrors = [{
                message: 'URL changed during save'
            }];
            
        } else {
            //parse the page errors from the XHR
            try {
                pageErrors = JSON.parse(xhr.responseText);
            } catch(e) {
                console.warn(e);
                console.error('Error occurred while parsing responseText: ' + xhr.responseText);
                pageErrors = [{
                    message: 'Error occurred while contacting server. Are you logged in and is your session still active?'
                }];
            }
        }
        
        this._showFormErrors(pageErrors);
    },
    _onSuccess: function() {
        var dialog = this;
        this.$saveButtonText.text('Saved!');
        this.$formIndicator.removeClass('submitIndicator').addClass('successIndicator').delay(2000).queue(function() {
            dialog._dialogCancel();
            $.dequeue(this);
        });

        this.$('.page-error-container').empty().hide()
    },
    _onSave: function() {
        this.$formElements.attr('disabled','disabled');
        this.$richTextEditor.attr('contenteditable','false');
        this.$formIndicator.removeClass('successIndicator').removeClass('errorIndicator').addClass('submitIndicator').show();
        this.$saveButtonText.text('Saving...');
        this.$cancelButton.hide();
        
        var fetchingAttributes = this._getFormAttributes()
            .done(_.bind(function(values) {
                //remove any attributes that have the same values as the model
                _.each(_.keys(values), _.bind(function(propertyName) {
                    if(this.model.get(propertyName) === values[propertyName]) {
                        delete values[propertyName];
                    }
                }, this));
            
                //save the values to the server
                this.model.save(values, {
                    wait: true,
                    success: _.bind(this._onSuccess, this), 
                    error: _.bind(this._onSaveError, this)
                });
            },this))
            .fail(_.bind(function(msg) {
                this._showFormErrors([{
                    message: msg || 'Failed to retrieve form values'
                }]);
            }, this));
    },
    _getFormAttributes: function() {
        var values = {};
        
        values[nameSpace + 'Subject__c'] = this.$('input#subjectInput').val();
        values[nameSpace + 'Status__c'] = this.$('select#statusInput').val();
        values[nameSpace + 'Details__c'] = this.$('textarea#descriptionInput').val();
        values[nameSpace + 'Details_and_Steps_to_Reproduce__c'] = this.$('div#detailsAndStepsInput').html();
        
        var lookupFailHandler = function(label) {
            return function(lookupComponent, message) {
                return label + ': ' + message;
            };
        };
        
        var userLookupPromise = this.userLookupComponent.commit().done(_.bind(function(lookupComponent, user) {
            values[nameSpace + 'Assignee__c'] = (user && user.Id) ? user.Id : null; 
        },this)).then(void 0, lookupFailHandler('Assignee'));
        
        var sprintLookupPromise = this.sprintLookupComponent.commit().done(_.bind(function(lookupComponent, sprint) {
            values[nameSpace + 'Sprint__c'] = (sprint && sprint.Id) ? sprint.Id : null;
        },this)).then(void 0, lookupFailHandler('Sprint'));
        
        var scheduledBuildLookupPromise = this.buildLookupComponent.commit().done(_.bind(function(lookupComponent, build) {
            values[nameSpace + 'Scheduled_Build__c'] = (build && build.Id) ? build.Id : null;
        },this)).then(void 0, lookupFailHandler('Scheduled Build'));
        
        values[nameSpace + 'Story_Points__c'] = this.$('input#storyPointsInput').val();

        return $.when(userLookupPromise, sprintLookupPromise, scheduledBuildLookupPromise).then(function() { return values; });
    }
});

