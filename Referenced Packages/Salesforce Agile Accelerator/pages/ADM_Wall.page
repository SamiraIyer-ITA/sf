gus.WorkModel.fromData = function(records) {
    if(!_.isArray(records)) {
        records = [records];
    }
    
    var cache = gus.getCache(gus.WorkModel);
    var models = [];
    _.each(records, function(record) {
        
        if(record[nameSpace + 'Theme_Assignments__r']) {
            var themeCache = gus.getCache(gus.ThemeModel);
       _.each(record[nameSpace + 'Theme_Assignments__r'], function(themeAssignment) {
       	themeCache.add(themeAssignment[nameSpace + 'Theme__r']);
       	themeAssignment[nameSpace + 'Theme__r'] = themeCache.get(themeAssignment[nameSpace + 'Theme__c']);
            });
        }
        
        if(record[nameSpace + 'Scheduled_Build__r']) {
            gus.getCache(gus.BuildModel).add(record[nameSpace + 'Scheduled_Build__r'], {merge: true});
            delete record[nameSpace + 'Scheduled_Build__r'];
        }
        
        if(record[nameSpace + 'Assignee__r']) {
            gus.getCache(gus.UserModel).add(record[nameSpace + 'Assignee__r'], {merge: true});
            delete record[nameSpace + 'Assignee__r'];
        }
        
        if(record[nameSpace + 'QA_Engineer__r']) {
            gus.getCache(gus.UserModel).add(record[nameSpace + 'QA_Engineer__r'], {merge: true});
            delete record[nameSpace + 'QA_Engineer__r'];
        }

        if(record[nameSpace + 'Product_Owner__r']) {
            gus.getCache(gus.UserModel).add(record[nameSpace + 'Product_Owner__r'], {merge: true});
            delete record[nameSpace + 'Product_Owner__r'];
        }

        if(record[nameSpace + 'Sprint__r']) {
            gus.getCache(gus.SprintModel).add(record[nameSpace + 'Sprint__r'], {merge: true});
            delete record[nameSpace + 'Sprint__r'];
        }
        
        if(record[nameSpace + 'Tasks__r']) {
            record[nameSpace + 'Tasks__r'] = gus.TaskModel.fromData(record[nameSpace + 'Tasks__r']);
        }
        
        cache.add(record, {merge:true});
        models.push(cache.get(record.Id));
    });
    return models;
};  
gus.TaskModel.fromData = function(records) {
    if(!_.isArray(records)) {
        records = [records];
    }
    
    var cache = gus.getCache(gus.TaskModel);
    return _.map(records, function(record) {
        if(record.Assigned_To__r) {
            gus.getCache(gus.UserModel).add(record.Assigned_To__r, {merge:true});
            delete record.Assigned_To__r;
        }
        
        cache.add(record, {merge:true});
        return cache.get(record.Id);
    });
};
