        gus.Window.on('exitFullscreen', _.bind(this.fullscreenTurnOff, this));
        
        //retrieve the team associated to the team ID
        var panelContainer = this;
        this.teamDeferred = gus.getCache(gus.TeamModel).getOrFetch(options.teamId)
