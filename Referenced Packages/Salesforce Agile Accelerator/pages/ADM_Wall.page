    containsPanelWith: function(f) {
        return _.any(this.panels, f);
    },
    
    getPanelWith: function(f) {
        return _.find(this.panels, f);
    },
    
    containsPanelOfType: function(type) {
        return this.containsPanelWith(function(panel) {
            return panel instanceof type;
        });
    },
    
    getPanelOfType: function(type) {
        return this.getPanelWith(function(panel) {
            return panel instanceof type;
        });
