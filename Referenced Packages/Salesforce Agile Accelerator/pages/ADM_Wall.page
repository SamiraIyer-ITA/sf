                if(this.columns.length == 0){ //if columns array is defined but has no columns
                this.columns = defaultColumns;
                }

                if(editedColumn.visible == true){ //add selected column to the JSON
                this.columns.push(editedColumn.label.toUpperCase());
                }
                else{ //if it must be removed then remove from JSON also
                var index = this.columns.indexOf(editedColumn.label.toUpperCase());
                if(index > -1){
                this.columns.splice(index,1);
                }
                }
                }
                else{  //if no columns then add the default columns & the selected column
