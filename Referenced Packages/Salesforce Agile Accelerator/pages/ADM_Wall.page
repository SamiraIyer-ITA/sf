        this.loadingPanels = true;
        
        //lets go ahead load the starting panels
        var panelLoadingDeferred = panelRouter.start();
        panelLoadingDeferred.always(_.bind(function() {
            this.loadingPanels = false;
            this.$('.loading-notification').hide();
            this.$('#panel-list').show();
