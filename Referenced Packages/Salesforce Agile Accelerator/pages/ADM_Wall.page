        } else {
            this.$el.removeAttr('data-checked');
        }
        return this;
    },
    _onMenuItemClick: function(event) {
        event.stopPropagation();
        if(this.checked) {
            this.setChecked(false);
        } else {
            this.setChecked(true);
        }
    }
});

gus.dataTable.Column = function(options) { 
    this.cid = _.uniqueId('column_');
    this.initialize.apply(this, arguments);
};
_.extend(gus.dataTable.Column.prototype, Backbone.Events, {
    initialize: function(options) {
        options = _.defaults(options || {}, {visible: true, hideable: true, index: -1});
        this.id = options.id || this.cid;
        
        //label: the value that is used for UI to be displayed to user 
        this.label = options.label;
        
        //visible: if true, then the column is visible otherwise it is hidden
        this.visible = options.visible;
        
        //hideable: if true, the column is able to be hidden by the user
        this.hideable = options.hideable;
        
        //tableView: the table to which the column is associated to
        this.tableView = options.tableView;
        this.columns = options.columns;
        
        //tableHiddenClass: the class that is applied to the table when the
        //column should be hidden
        this.tableHiddenClass = options.tableHiddenClass || this.id + '-hidden';
        
        //index: the index of the column within the group of columns
        this.index = options.index;
        
        this.on('change:visibility', _.bind(this.apply, this));
    },
    hide: function() {
        if(!this.visible) {
            return;
        }
        this.visible = false;
        this.trigger('change:visibility');
    },
    show: function() {
        if(this.visible) {
            return;
        }
        this.visible = true;
        this.trigger('change:visibility');
    },
    
    /**
     * Applies the current visibility to the columns and menu item.
     */
    apply: function() {
        if(this.visible) {
            this.tableView.$el.removeClass(this.tableHiddenClass);
        } else {
            this.tableView.$el.addClass(this.tableHiddenClass);
        }
    }
});
gus.dataTable.Column.extend = Backbone.View.extend;

