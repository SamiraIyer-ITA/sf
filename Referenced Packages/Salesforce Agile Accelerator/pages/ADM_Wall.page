    hide: function() {
        if(this.visible) {
            this.visible = false;
            this.trigger('change:visible', this.visible);
        }
    },
    
    //gets the button with the specified ID. returns undefined if
    //the button is not registered
    get: function(buttonId) {
        return this.buttonsById[buttonId];
    },
    
    //disables a button
    disableButton: function(buttonId) {
        if(this.buttonsById[buttonId]) {
            this.buttonsById[buttonId].disable();
        }
    },
    
    //enables a button
