        var buildEqualTo = function(build) {
            return function(panel){ 
                return panel.build && panel.build.id === build.id; 
            };
        };
        if(this.containsPanelWith(buildEqualTo(build))) {
            return $.when(this.getPanelWith(buildEqualTo(build)));
        }
        
