    initialize: function(options) {
    try{
        this.panels = [];
        
        this.teamId = options.teamId;
        uwTeamId = this.teamId;
        
        if(!this.teamId) {
            throw 'Unable to load the team because teamId is not set';
        }
        
        //create the "Add Panel" menu
        this.addPanelMenu = new gus.menu.Menu({
            buttons: [
                {id: 'backlog'},
                {id: 'backburner'},
                {id: 'epic'},
                {id: 'featureBacklog'},
                {id: 'trustBacklog'},
                {id: 'sprintBacklog'},
                {id: 'themeBacklog'},
                {id: 'buildBacklog'}
            ]
        });
        
        //bind internal events
        this.addPanelMenu.on('change:visible', this._updateAddPanelButton, this);
        this.addPanelMenu.on('change:visible', this._updateAddPanelMenu, this);
        this._initializeAddPanelMenu();
        
        //bind external events
        $(window).on('resize', _.bind(_.throttle(this.resize, 100), this));
