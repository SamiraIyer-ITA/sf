        // sets the min-width on headers so table doesn't shift
        this.$el.closest('ul').find('li.headers > div:visible').each(function(i) {
            $(this).css('min-width', $(this).width() + 'px');
        });

    },
    isSelected: false,
    toggleSelect: function() {
        if(!_.result(this, 'canSelect')) {
            return;
        }
        
        if(this.isSelected) {
            this.unselect();
        } else {
            this.select();
        }
    },
    select: function() {
        if(!_.result(this, 'canSelect')) {
            return;
        }
        
        if(!this.isSelected) {
            this.isSelected = true;
            this.$el.addClass('selected');
            this.trigger('select');
        }
    },
    unselect: function() {
        if(!_.result(this, 'canSelect')) {
            return;
        }
        
        if(this.isSelected) {
            this.isSelected = false;
            this.$el.removeClass('selected');
            this.trigger('unselect');
        }
    },
    toggleCheck: function() {
        if(this.$el.hasClass('selected')) {
            this.uncheck();
        } else {
            this.check();
        }
    },
    check: function() {
        if (!this.$el.hasClass('selected')) {
            this.trigger('check');
            this.$el.addClass('selected');
        }
    },
    uncheck: function() {
        if (this.$el.hasClass('selected')) {
            this.trigger('uncheck');
            this.$el.removeClass('selected');
        }
    },
    setTasks: function(tasks) {
        this.taskColumns.setTasks(tasks);
    },
    _onError: function(options) {
        if(_.isString(options)) {
            options = {message: options};
        }
        console.error(options.message);
        this.trigger('error', options);
    }
});

gus.WorkCollection = Backbone.Collection.extend({
    initialize: function(models, options) {
        options = _.defaults(options || {}, {batchSize: 50, model: gus.WorkModel});
        gus.WorkCollection.__super__.initialize.call(this, models, options);
        
        //the remote GET method
        this.remoteGetMethod = options.remoteGetMethod;
        
        //batch loading properties
        this.batchSize = options.batchSize;
        this.currentBatch = -1;
        
        //total number of records on the server
        this.numberOfRecords = -1;
    },
    
    /**
     * Overriding the basic fetch function
     */
    fetch: function() {
        return this.fetchNextBatch();
    },
    
    /**
     * Fetches the next batch of records from the server.
     * @return a $.Deferred
     */
    fetchNextBatch: function(options) {
        options = _.defaults(options || {}, {batchSize: this.batchSize});
        
        //we only allow one fetch at a time, no matter what options are specified
        if(!this.promise) {
            
            //calculate the next index
            options.offset = (this.currentBatch + 1) * options.batchSize;
            
            this.promise = this.remoteGetMethod(options);
            this.promise = this.promise.then(_.bind(function(data) {
                this.setNumberOfRecords(data.numberOfRecords);
                this.currentBatch += 1;
                
                delete this.promise;
                
                this.add(data.records, {at: options.offset, merge: true});
                this.trigger('addBatch', data.records);
                return this;
            }, this));
        }
        
        return this.promise;
    },
    
    /**
     * Determines if all of the records have been loaded
     */
    hasMoreRecords: function() {
        if(_.isUndefined(this.numberOfRecords) || this.numberOfRecords == -1) {
            return void 0;
        }
        return this.length < this.numberOfRecords;
    },
    
    setNumberOfRecords: function(value) {
        if(value != this.numberOfRecords) {
            this.numberOfRecords = value;
            this.trigger('change:numberOfRecords');
        }
    }
});

