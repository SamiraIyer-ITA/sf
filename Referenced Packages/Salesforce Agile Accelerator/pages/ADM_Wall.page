        this.setElement(dialog);
        var view = this;
        
        $('<a href="/' + this.model.get('Id') + '" title="Popout" class="ui-dialog-titlebar-extlink ui-corner-all" role="button"><span class="ui-icon ui-icon-extlink">popout</span></a>').appendTo(this.$('.ui-dialog-titlebar'));

