        }
    },
    saveOrder: function(changes) {},
        // This method only gets called on the panel
        // that receives a DnD from another panel
    _onSortReceive: function(event, ui) {
        this.processDrop(this);
        this.autoLoadRecordsEnabled = true;
        this.renderRows();  
    },
    _onSortStart: function(event, ui) {
        var allowedLists = this.$('ul.wall-work-list').sortable('option','connectWith');
        $('ul.wall-work-list').not(this.$('ul.wall-work-list')).not(allowedLists).not('.themepanel ul.wall-work-list').sortable('disable');
        $('.themepanel ul.wall-work-list').addClass('ui-sortable-disabled');

        //due to an issue with the jquery sortable, we disable autoloading of records
        this.autoLoadRecordsEnabled = false;
        itemsBeingSorted = this.$('li.selected a');
        this.$('li.selected').css('display','none');

        // make placeholder row (and cells inside row), number of cells based on number of visible columns
        var placeholderHtml = '';
        this.$('li.headers > div:visible').each(function(i) {
            placeholderHtml += '<div></div>';
        });
        this.$('li.placeholder').html(placeholderHtml);     

        // record counter on cursor start
        if (this.$('li.selected').size() != 1) { var sNeeded = 's' } else { var sNeeded = '' }
        $('#cursorInfo').html(this.$('li.selected').size() + ' record' + sNeeded).show();
        numberOfRecordsSelected = this.$('li.selected').size();
        
        // TODO: recalculate height of panel after items are grabbed and hidden
    },
    _onSortBeforeStop: function(event, ui) {
            // remove min-width on visible headers so table can flex (if needed)
            this.$('li.headers > div:visible').each(function(i) {
                $(this).css('min-width','0px');
            });
    
            $('ul.wall-work-list').not('.themepanel ul.wall-work-list').sortable('enable');
            $('.themepanel ul.wall-work-list').removeClass('ui-sortable-disabled');

            // Record the panel which was dropped on as well as the index within the items on that panel
            droppedPanel = ui.placeholder.closest('.panel');
            droppedIndex = droppedPanel.find('ul.wall-work-list > li:visible').not('.selected').not('.headers').index(ui.placeholder);

            // Create a list of Backbone models of the selected items which are being
            // dragged and dropped
            var collection = this.collection;
            modelsBeingSorted = new Array();
            itemsBeingSorted.each(function(idx, item) {
    
            // Strip off the leading "/"
            var work_id = $(item).attr("href").substr(1);
    
            // Create a list of the models being sort for later insertion
            var model = collection.get(work_id);
            modelsBeingSorted.push(model);
        });
    $('#cursorInfo').html(null).hide();
    },
    _onSortStop: function(event, ui) {
        var collection = this.collection;
        // Don't remove items from the Trust Backlog panel
        if (this.$el.attr('class') != 'trustbacklogpanel') {
            collection.remove(modelsBeingSorted, {silent: true});
        }

        // If this panel is the target of the drag and drop, add
        // the items back in in the new position
        // (Note: if the target of the DnD is another panel, the
        // call to processDrop() is handled in the receive event, which
        // is only triggered in the other panel).  Basically, this code
        // handles the case when the DnD is within the same panel.
        if (this.$el.attr('id') == droppedPanel.attr('id')) {
            this.processDrop(this);
        }

        //enable the autoloading of records
        this.autoLoadRecordsEnabled = true;

        this.$('li.selected').removeAttr('style');
        var newNumberOfRecords = this.collection.numberOfRecords - numberOfRecordsSelected
        this.collection.setNumberOfRecords(newNumberOfRecords);
        this.renderRows();
    },
    processDrop: function(dropTarget) {
        // This method handles both the case of a DnD within the same panel
        // and also across two different panels
        var newIndex = droppedIndex;
		// Put the models back in to the collection at the dropped panel
        dropTarget.collection.add(modelsBeingSorted, {at: newIndex, silent: true});
        
        //set the new ranks (specific to the target panel) within each of the models in the collection
        // and also update other fields depending on the drop target
        var listView = dropTarget;
        var changes = [];
        var index = 0;
        dropTarget.collection.each(function(model) {
            var rank = index + 1;
            var change = {Id: model.get('Id')};
            change[listView.sortedProperty] = rank;
            model.set(listView.sortedProperty, rank);
            // If the drop target is a sprint panel, assign the sprint id
            // if necessary
            if (dropTarget.$el.attr('id').indexOf('sprintpanel') != -1) {
                if (model.get('Sprint__c') != dropTarget.sprintId) {
                    change['Sprint__c'] = dropTarget.sprintId;
                    model.set('Sprint__c', dropTarget.sprintId);
                }
            }
            
            
            if (dropTarget.$el.attr('id').indexOf('epicpanel') != -1) {
                    if (model.get('Epic__c') != dropTarget.epicId) {
                        change['Epic__c'] = dropTarget.epicId;
                        model.set('Epic__c', dropTarget.epicId);
                    }
                }
            
            if (dropTarget.$el.attr('id').indexOf('scheduledbuildpanel') != -1) {
                if (model.get('Scheduled_Build__c') != dropTarget.buildId) {
                    change['Scheduled_Build__c'] = dropTarget.buildId;
                    model.set('Scheduled_Build__c', dropTarget.buildId);
                }
            }
            
            if (dropTarget.$el.attr('id').indexOf('backburnerpanel') != -1) {
                change[nameSpace + 'Priority_Rank__c'] = null;     //Backlog Rank to null
                model.set(nameSpace + 'Priority_Rank__c', null);
            }
            
            changes.push(change);
           
            index++;
        });
        
        dropTarget.collection.sort();
        
        //send the rank order (and possibly other updated attributes) back to the server
        dropTarget.saveOrder(changes);
        
        dropTarget.$('li.selected').removeAttr('style');

    },

    /**
     * Event handler when the table-container element is scrolled
     */
    _onTableContainerScroll: function() {
        
        //if auto loading the next batch of records
        if(this.autoLoadRecordsEnabled && !this.autoLoadRecords) {
            var $container = this.$('.table-container');
            var containerHeight = $container.height();
            var scrollTop = $container.scrollTop();
            var scrollBottom = scrollTop + containerHeight;
            
            var contentHeight = this._getContentHeight();
            
            //once the user scrolls past this value, then we need to showMore
            var scrollRangeTop = contentHeight - (contentHeight / 10);
            
            //when the user scrolls down to within the last 10% of the table-container, automatically
            //load the next batch of records
            if(scrollBottom >= scrollRangeTop) {
                this.autoLoadRecords = true;
                var promise = this.showMore();
                promise.always(_.bind(function(){
                    delete this.autoLoadRecords;
                }, this));
            }
        }
    },
    
    /**
     * Applies the special 'lastcolumnvisible' to the last visible columns within the row.
     */
    _applyLastVisibleColumn: function(context) {
        if(!context) {
            context = this.$('li.work');
        }
        
        context.find('>div').removeClass('lastvisiblecolumn');
        
        //TODO: this doesn't use this.lastVisibleColumn so the last one may not be correct
        context.find('>div:visible:last').addClass('lastvisiblecolumn');
    },
    
    /**
     * Determines the last visible column
     */
    _checkLastVisibleColumn: function() {
        var columns = _.values(this.columns);
        var lastVisibleColumn = null;
        for(var index = columns.length - 1; index > -1; index--) {
            if(columns[index].visible) {
                lastVisibleColumn = columns[index];
                break;
            }
        }
        return lastVisibleColumn;
    },
    
    _onError: function(message, event) {
        if(this.panelContainer) {
            this.panelContainer._onError(message, event);
        } else {
            throw new Error(message);
        }
    }
});

