    if(panelTitle== 'BACKBURNER' || panelTitle == 'BACKLOG'){
    return panelTitle;
    }
    else if((panelTitle.indexOf('FEATURE') != -1 || panelTitle.indexOf('TRUST') != -1) && (panelTitle.indexOf('THEME') == -1)){
    var trimTitle = panelTitle.split(' ');
    return trimTitle[0];
    }
    else if(panelTitle.indexOf('BUILD') != -1  && panelTitle.indexOf('THEME') == -1){
    return 'build'+'('+this.buildId+')';

    } 
    else if(panelTitle.indexOf('SPRINT') != -1  && panelTitle.indexOf('THEME') == -1){
    return 'sprint'+'('+this.sprintId+')';

    }
    else if(panelTitle.indexOf('EPIC') != -1  && panelTitle.indexOf('THEME') == -1){
    return 'epic'+'('+this.epicId+')';

    }
    else if(panelTitle.indexOf('THEME') != -1){
    return 'theme'+'('+this.themeId+')';

    }
    else{
    return panelTitle;
    }
