            <guestUser>Trade_Community</guestUser>
        </sharedTo>
        <criteriaItems>
            <field>OwnerId</field>
            <operation>notEqual</operation>
            <value>1234567890</value>
        </criteriaItems>
    </sharingGuestRules>
</SharingRules>
