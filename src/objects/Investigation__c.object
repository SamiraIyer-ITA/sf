    <validationRules>
        <fullName>A0601PermSetReparentsInvestigation</fullName>
        <active>true</active>
        <description>Investigation records can only be reparented by members of ADCVD_App_Modify_Parents permission set.</description>
        <errorConditionFormula>AND(
            $Setup.Master_Automation__c.ValidationRulesEnabled__c == true,
            Locked__c == false,
            NOT( ISNEW() ),
            $RecordType.DeveloperName = &quot;AD&quot;,
            OR(
            ISCHANGED(ADCVD_Case__c),
            ISCHANGED(Petition__c)
            ),
            NOT(
            $Permission.ADCVD_App_Modify_Parents
            )
            )</errorConditionFormula>
        <errorMessage>You are not authorized to edit ADCVD Case or Petition.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>A0602PermSetReparentsInvestigation</fullName>
        <active>true</active>
        <description>Investigation records can only be reparented by members of ADCVD_App_Modify_Parents permission set.</description>
        <errorConditionFormula>AND(
            $Setup.Master_Automation__c.ValidationRulesEnabled__c == true,
            Locked__c == false,
            NOT( ISNEW() ),
            $RecordType.DeveloperName = &quot;CVD&quot;,
            OR(
            ISCHANGED(ADCVD_Case__c),
            ISCHANGED(Petition__c)
            ),
            NOT(
            $Permission.ADCVD_App_Modify_Parents
            )
            )</errorConditionFormula>
        <errorMessage>You are not authorized to edit ADCVD Case or Petition.</errorMessage>
    </validationRules>
