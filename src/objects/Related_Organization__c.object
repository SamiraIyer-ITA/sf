    <validationRules>
        <fullName>A1001PermSetReparentsRelatedOrg</fullName>
        <active>true</active>
        <description>Related Organization records can only be reparented by members of ADCVD_App_Modify_Parents permission set.</description>
        <errorConditionFormula>AND(
			$Setup.Master_Automation__c.ValidationRulesEnabled__c == true,
			Locked__c == false,
			NOT( ISNEW() ),
			$RecordType.DeveloperName = &quot;Customs_Instructions_Only&quot;,
			OR(
			ISCHANGED(ADCVD_Case__c),
			ISCHANGED(Litigation__c),
			ISCHANGED(Investigation__c),
			ISCHANGED(Petition__c),
			ISCHANGED(Custom_Instruction__c),
			ISCHANGED(Segment__c)
			),
			NOT(
			$Permission.ADCVD_App_Modify_Parents
			)
			)</errorConditionFormula>
        <errorMessage>You are not authorized to edit ADCVD Case, Petition, Litigation, Investigation, Custom Instruction, or Segment.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>A1002PermSetReparentsRelatedOrg</fullName>
        <active>true</active>
        <description>Related Organization records can only be reparented by members of ADCVD_App_Modify_Parents permission set.</description>
        <errorConditionFormula>AND(
			$Setup.Master_Automation__c.ValidationRulesEnabled__c == true,
			Locked__c == false,
			NOT( ISNEW() ),
			$RecordType.DeveloperName = &quot;Petitioner_Respondent&quot;,
			OR(
			ISCHANGED(ADCVD_Case__c),
			ISCHANGED(Litigation__c),
			ISCHANGED(Investigation__c),
			ISCHANGED(Petition__c),
			ISCHANGED(Custom_Instruction__c),
			ISCHANGED(Segment__c)
			),
			NOT(
			$Permission.ADCVD_App_Modify_Parents
			)
			)</errorConditionFormula>
        <errorMessage>You are not authorized to edit ADCVD Case, Petition, Litigation, Investigation, Custom Instruction, or Segment.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_Edit_Locked_Record</fullName>
        <active>true</active>
        <errorConditionFormula>AND( $Permission.Edit_ADCVD_Locked_Records == false, 
NOT(ISNEW()), 
PRIORVALUE(Locked__c) == true 
)</errorConditionFormula>
        <errorMessage>This record is locked.  Please contact your administrator about unlocking this record.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Custom_Instructions_must_have_a_parent</fullName>
        <active>true</active>
        <description>Custom Instructions must have a parent (ADCVD Order, Investigation, Petition, Segment, or Suspension Agreement)</description>
        <errorConditionFormula>AND($Setup.Master_Automation__c.ValidationRulesEnabled__c == true, Locked__c == false,AND(NOT(ISBLANK(Custom_Instruction__c)), ISBLANK( Segment__c ),  ISBLANK( ADCVD_Order__c ), ISBLANK( Petition__c ),  ISBLANK( Investigation__c ),  ISBLANK( Segment__r.Suspension_Agreement__c )))</errorConditionFormula>
        <errorMessage>Custom Instructions must have a parent.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Ensure_there_is_only_one_parent</fullName>
        <active>true</active>
        <description>This validation rule makes sure you cannot create a Interested Organization without a Investigation, Petition, Orders, Litigation or Segment attached to it. If all three of these lookup fields are empty, the validation error message will trigger.</description>
        <errorConditionFormula>AND(
    $Setup.Master_Automation__c.ValidationRulesEnabled__c == true, 
    Locked__c == false, 
    (	
	IF(ISBLANK(Petition__c),0,1)+ 
	IF(ISBLANK(Segment__c),0,1)+ 
        IF(ISBLANK(ADCVD_Order__c),0,1)+
	IF(ISBLANK(Investigation__c),0,1)+ 
	IF(ISBLANK(Litigation__c),0,1) &lt;&gt;1
   )
)</errorConditionFormula>
        <errorMessage>You must have only one of the following: Petition, Investigation,  ADCVD Order, Litigation, or Segment associated to this Interested Organization.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Interested_Org_must_have_a_parent</fullName>
        <active>true</active>
        <description>Interested Organizations without a Custom Instructions must have an ADCVD Order, Investigation, Petition, Segment or a related ADCVD Case. If Customs Instructions is the parent, a Segment record must also be included as a reference.</description>
        <errorConditionFormula>AND(
  $Setup.Master_Automation__c.ValidationRulesEnabled__c == true, Locked__c == false,    
  OR(
     IF(NOT(ISBLANK(ADCVD_Order__c)),1,0) +
     IF(NOT(ISBLANK(Investigation__c)),1,0) +
     IF(NOT(ISBLANK(Litigation__c)),1,0) +
     IF(NOT(ISBLANK(Petition__c)),1,0) +
     IF(NOT(ISBLANK(Segment__c)),1,0) &lt;&gt; 1
    ,
     AND(NOT(ISBLANK(Custom_Instruction__c)),ISBLANK(Segment__c))
   )
 )</errorConditionFormula>
        <errorMessage>Interested Organizations are required to have a parent, and only one parent should be entered. If Customs Instructions are the parent, a Segment must also be referenced.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Only_Admin_can_reparent_Interested_Org</fullName>
        <active>false</active>
        <description>Only Admin can re-parent an Interested Organization.</description>
        <errorConditionFormula>AND($Setup.Master_Automation__c.ValidationRulesEnabled__c == true, Locked__c == false,
   AND(NOT(ISNEW()), $Profile.Name != &apos;System Administrator&apos;,
    OR( 
       ISCHANGED(ADCVD_Case__c), 
       ISCHANGED(Petition__c),
       ISCHANGED(Investigation__c),
       ISCHANGED(ADCVD_Order__c),  
       ISCHANGED(Segment__c), 
       ISCHANGED(Custom_Instruction__c)  
     ) 
   )
 )</errorConditionFormula>
        <errorMessage>Only Admin can re-parent an Interested Organization.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Only_Admin_reparents_Related_Org</fullName>
        <active>false</active>
        <description>Interested Organization records can only be reparented by a System Administrator.</description>
        <errorConditionFormula>AND(
    $Setup.Master_Automation__c.ValidationRulesEnabled__c == true,
    Locked__c == false,
    AND(
        $Profile.Name != &apos;System Administrator&apos;,
        NOT($Permission.ADCVD_App_Modify_Parents),
        OR(
            ISCHANGED( Investigation__c ),
            ISCHANGED( Petition__c ),
            ISCHANGED( Segment__c ),
            ISCHANGED( ADCVD_Order__c ),
            ISCHANGED( Litigation__c )
        )
    )
)</errorConditionFormula>
        <errorMessage>Related Organization records can only be reparented by a System Administrator.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Segment_must_be_populated_if_ci_is</fullName>
        <active>true</active>
        <description>Segment must be populated if Custom Instruction is a parent</description>
        <errorConditionFormula>AND(
    $Setup.Master_Automation__c.ValidationRulesEnabled__c == true,
    Locked__c == false,
    AND(
        NOT(ISBLANK( Custom_Instruction__c )), ISBLANK( Segment__c )
    )
)</errorConditionFormula>
        <errorMessage>You must have a segment if you have Custom Instructions.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
