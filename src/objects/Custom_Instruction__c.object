    <validationRules>
        <fullName>A0301PermSetReparentsCustomInstr</fullName>
        <active>true</active>
        <description>Custom Instruction records can only be reparented by members of ADCVD_App_Modify_Parents permission set.</description>
        <errorConditionFormula>AND(
			$Setup.Master_Automation__c.ValidationRulesEnabled__c == true,
			Locked__c == false,
			NOT( ISNEW() ),
			OR(
            ISCHANGED(ADCVD_Case__c),
            ISCHANGED(Investigation__c),
			ISCHANGED(ADCVD_Order__c),
			ISCHANGED(Petition__c),
			ISCHANGED(Segment__c),
			ISCHANGED(Suspension_Agreement__c)
			),
			NOT(
			$Permission.ADCVD_App_Modify_Parents
			)
			)</errorConditionFormula>
        <errorMessage>You are not authorized to edit ADCVD Case, ADCVD Order, Petition, Investigation, Segment, or Suspension Agreement.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_Edit_Locked_Record</fullName>
        <active>true</active>
        <errorConditionFormula>AND( $Permission.Edit_ADCVD_Locked_Records == false,
NOT(ISNEW()),
OR (Locked__c == true,
PRIORVALUE(Locked__c) == true
)
)</errorConditionFormula>
        <errorMessage>This record is locked.  Please contact your administrator about unlocking this record.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Ensure_there_is_only_one_parent</fullName>
        <active>true</active>
        <description>This validation rule makes sure you cannot create a Custom Instruction without a Investigation, Petition, Orders, Suspension Agreement, or Segment attached to it. If all three of these lookup fields are empty, the validation error message will trigger.</description>
        <errorConditionFormula>AND(
    $Setup.Master_Automation__c.ValidationRulesEnabled__c == true,
    Locked__c == false,
    (
	IF(ISBLANK(Petition__c),0,1)+
	IF(ISBLANK(Segment__c),0,1)+
        IF(ISBLANK(ADCVD_Order__c),0,1)+
	IF(ISBLANK(Investigation__c),0,1)+
	IF(ISBLANK(Suspension_Agreement__c),0,1) &lt;&gt;1
   )
)</errorConditionFormula>
        <errorMessage>You must have only one of the following: Petition, Investigation,  ADCVD Order, Suspension Agreement, or Segment associated to this Custom Instruction.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Only_Admin_reparents_Custom_Instruction</fullName>
        <active>false</active>
        <description>Custom Instruction records can only be reparented by a System Administrator.</description>
        <errorConditionFormula>AND(
    $Setup.Master_Automation__c.ValidationRulesEnabled__c == true,
    Locked__c == false,
    AND(
        $Profile.Name != &apos;System Administrator&apos;,
        NOT($Permission.ADCVD_App_Modify_Parents),
        OR(
            ISCHANGED( Investigation__c ),
            ISCHANGED( Petition__c ),
            ISCHANGED( Segment__c ),
            ISCHANGED( ADCVD_Order__c ),
            ISCHANGED( Suspension_Agreement__c )
        )
    )
)</errorConditionFormula>
        <errorMessage>Custom Instruction records can only be reparented by a System Administrator.</errorMessage>
    </validationRules>
